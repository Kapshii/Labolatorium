<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="rootPane1" prefHeight="250.0" prefWidth="386.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Labolatorium.addmember.MemberAddController">
    <stylesheets>
        <URL value="@member_add.css" />
    </stylesheets>
   <children>
      <VBox prefHeight="250.0" prefWidth="386.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="name" focusColor="#1976d2" promptText="Imię">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="id" focusColor="#1976d2" layoutX="10.0" layoutY="10.0" promptText="ID Użytkownika">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="mobile" focusColor="#1976d2" layoutX="10.0" layoutY="35.0" promptText="Mobilny">
               <VBox.margin>
                  <Insets left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="email" focusColor="#1976d2" layoutX="10.0" layoutY="60.0" promptText="Email">
               <VBox.margin>
                  <Insets bottom="20.0" left="10.0" right="10.0" top="20.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="50.0" prefWidth="386.0">
               <children>
                  <JFXButton fx:id="saveButton" onAction="#addMember" prefHeight="50.0" prefWidth="200.0" stylesheets="@member_add.css" text="Zapisać" />
                  <JFXButton fx:id="cancelButton" onAction="#cancel" prefHeight="50.0" prefWidth="200.0" stylesheets="@member_add.css" text="Anulować" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
